{
	"name": "dataflow_sql_to_csv",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sql_songplays",
						"type": "DatasetReference"
					},
					"name": "songplaystable",
					"description": "get data from sql_songplays"
				},
				{
					"dataset": {
						"referenceName": "sql_albums",
						"type": "DatasetReference"
					},
					"name": "albumstable",
					"description": "get data from sql_albums"
				},
				{
					"dataset": {
						"referenceName": "sql_artists",
						"type": "DatasetReference"
					},
					"name": "artiststable",
					"description": "get data from sql_artists"
				},
				{
					"dataset": {
						"referenceName": "sql_genre",
						"type": "DatasetReference"
					},
					"name": "genretable",
					"description": "get data from sql_genre"
				},
				{
					"dataset": {
						"referenceName": "sql_labels",
						"type": "DatasetReference"
					},
					"name": "labelstable",
					"description": "get data from sql_labels"
				},
				{
					"dataset": {
						"referenceName": "sql_location",
						"type": "DatasetReference"
					},
					"name": "locationtable",
					"description": "get data from sql_location"
				},
				{
					"dataset": {
						"referenceName": "sql_songs",
						"type": "DatasetReference"
					},
					"name": "songstable",
					"description": "get data from sql_songs"
				},
				{
					"dataset": {
						"referenceName": "sql_users",
						"type": "DatasetReference"
					},
					"name": "userstable",
					"description": "get data from sql_users"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "csv_songplays",
						"type": "DatasetReference"
					},
					"name": "songplayscsv",
					"description": "paste data to csv_songplays"
				},
				{
					"dataset": {
						"referenceName": "csv_albums",
						"type": "DatasetReference"
					},
					"name": "albumscsv",
					"description": "paste data to csv_albums"
				},
				{
					"dataset": {
						"referenceName": "csv_artists",
						"type": "DatasetReference"
					},
					"name": "artistscsv",
					"description": "paste data to csv_artists"
				},
				{
					"dataset": {
						"referenceName": "csv_genre",
						"type": "DatasetReference"
					},
					"name": "genrecsv",
					"description": "paste data to csv_genre"
				},
				{
					"dataset": {
						"referenceName": "csv_labels",
						"type": "DatasetReference"
					},
					"name": "labelscsv",
					"description": "paste data to csv_labels"
				},
				{
					"dataset": {
						"referenceName": "csv_location",
						"type": "DatasetReference"
					},
					"name": "locationcsv",
					"description": "paste data to csv_location"
				},
				{
					"dataset": {
						"referenceName": "csv_songs",
						"type": "DatasetReference"
					},
					"name": "songscsv",
					"description": "paste data to csv_songs"
				},
				{
					"dataset": {
						"referenceName": "csv_users",
						"type": "DatasetReference"
					},
					"name": "userscsv",
					"description": "paste data to csv_users"
				}
			],
			"transformations": [
				{
					"name": "exportdate",
					"description": "Add new column with export date"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SongPlayID as integer,",
				"          SongID as integer,",
				"          UserID as integer,",
				"          LocationID as integer,",
				"          StartTime as timestamp,",
				"          EndTime as timestamp,",
				"          {StartTime UTC} as string,",
				"          Latitude as string,",
				"          Longitude as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> songplaystable",
				"source(output(",
				"          AlbumID as integer,",
				"          ArtistID as integer,",
				"          Name as string,",
				"          ReleaseDate as date,",
				"          LabelID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> albumstable",
				"source(output(",
				"          ArtistID as integer,",
				"          Name as string,",
				"          DateCreated as date,",
				"          LocationID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> artiststable",
				"source(output(",
				"          GenreID as integer,",
				"          Name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> genretable",
				"source(output(",
				"          LabelID as integer,",
				"          Name as string,",
				"          LocationID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> labelstable",
				"source(output(",
				"          LocationID as integer,",
				"          TimeZone as string,",
				"          Country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> locationtable",
				"source(output(",
				"          SongID as integer,",
				"          ArtistID as integer,",
				"          AlbumID as integer,",
				"          Title as string,",
				"          Duration as timestamp,",
				"          ReleaseDate as date,",
				"          GenreID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> songstable",
				"source(output(",
				"          UserID as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Gender as string,",
				"          Email as string,",
				"          Password as string,",
				"          BirthDate as date,",
				"          DateCreated as date,",
				"          LocationID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> userstable",
				"songplaystable derive(ExportDate = currentUTC()) ~> exportdate",
				"exportdate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          SongPlayID as string,",
				"          SongID as string,",
				"          UserID as string,",
				"          LocationID as string,",
				"          StartTime as string,",
				"          EndTime as string,",
				"          {StartTime UTC} as string,",
				"          Latitude as string,",
				"          Longitude as string",
				"     ),",
				"     filePattern:'songplays[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          SongPlayID,",
				"          SongID,",
				"          UserID,",
				"          LocationID,",
				"          StartTime,",
				"          EndTime,",
				"          {StartTime UTC},",
				"          Latitude,",
				"          Longitude,",
				"          ExportDate",
				"     )) ~> songplayscsv",
				"albumstable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AlbumID as string,",
				"          ArtistID as string,",
				"          Name as string,",
				"          ReleaseDate as string,",
				"          LabelID as string",
				"     ),",
				"     filePattern:'albums[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          AlbumID,",
				"          ArtistID,",
				"          Name,",
				"          ReleaseDate,",
				"          LabelID",
				"     )) ~> albumscsv",
				"artiststable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ArtistID as string,",
				"          Name as string,",
				"          DateCreated as string,",
				"          LocationID as string",
				"     ),",
				"     filePattern:'artists[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          ArtistID,",
				"          Name,",
				"          DateCreated,",
				"          LocationID",
				"     )) ~> artistscsv",
				"genretable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          GenreID as string,",
				"          Name as string",
				"     ),",
				"     filePattern:'genre[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          GenreID,",
				"          Name",
				"     )) ~> genrecsv",
				"labelstable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          LabelID as string,",
				"          Name as string,",
				"          LocationID as string",
				"     ),",
				"     filePattern:'labels[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          LabelID,",
				"          Name,",
				"          LocationID",
				"     )) ~> labelscsv",
				"locationtable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          LocationID as string,",
				"          TimeZone as string,",
				"          Country as string",
				"     ),",
				"     filePattern:'location[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          LocationID,",
				"          TimeZone,",
				"          Country",
				"     )) ~> locationcsv",
				"songstable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          SongID as string,",
				"          ArtistID as string,",
				"          AlbumID as string,",
				"          Title as string,",
				"          Duration as string,",
				"          ReleaseDate as string,",
				"          GenreID as string",
				"     ),",
				"     filePattern:'songs[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          SongID,",
				"          ArtistID,",
				"          AlbumID,",
				"          Title,",
				"          Duration,",
				"          ReleaseDate,",
				"          GenreID",
				"     )) ~> songscsv",
				"userstable sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          UserID as string,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Gender as string,",
				"          Email as string,",
				"          Password as string,",
				"          BirthDate as string,",
				"          DateCreated as string,",
				"          LocationID as string",
				"     ),",
				"     filePattern:'users[].csv',",
				"     umask: 0772,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          UserID,",
				"          FirstName,",
				"          LastName,",
				"          Gender,",
				"          Email,",
				"          Password,",
				"          BirthDate,",
				"          DateCreated,",
				"          LocationID",
				"     )) ~> userscsv"
			]
		}
	}
}